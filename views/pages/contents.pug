extends ../layout/contents.pug

block styles
  link(rel="stylesheet", href="/css/contents.min.css")

block content
  .container.page-container
    .row
      .col-xs.breadcrumbs-container
        a.breadcrumb-item(href="/")
          |Etusivu
        each breadcrumb in breadcrumbs
          a.breadcrumb-item(href='/sisalto' + breadcrumb.path)
            =breadcrumb.title
        span.breadcrumb-item.active
          !=title
    .row
      .col-xs-5.page-nav
        h3
          |Kulttuuri ja kirjasto
      .col-xs-14
        .page-content
          h1
            !=title
          .featured-image(style='background-image: url(' + featuredImageSrc + '?size=750)')
          !=contents
      .col-xs-5.sidebar
        .sidebar-block
          h3
            |Palvelun tuottaa
          p
            |Yleiset kulttuuripalvelut
            br
            |Maaherrankatu 9-11 
            br
            |50100 Mikkeli PL 206
            br
            |50101 Mikkeli
